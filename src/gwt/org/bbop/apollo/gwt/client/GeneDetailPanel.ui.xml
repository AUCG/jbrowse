<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
        >
    <!--<g:HTMLPanel>-->
    <!--New Panel-->
    <!--</g:HTMLPanel>-->
    <ui:style>

        .container {
            margin-left: 20px;
            margin-top: 10px;
        }

        .widgetPanel {
        }

        
    </ui:style>
    <b:Container fluid="true" width="100%" styleName="{style.container}">
        <b:Row>
            <b:Column size="MD_4" styleName="{style.widgetPanel}">
            <g:Label text="Name"/>
            </b:Column>
            <b:Column size="MD_4" styleName="{style.widgetPanel}">
            <g:TextBox ui:field="nameField"/>
            </b:Column>
        </b:Row>
        <b:Row>
            <g:Label text="Symbol"/>
            <g:TextBox ui:field="symbolField"/>
        </b:Row>
    </b:Container>
</ui:UiBinder>